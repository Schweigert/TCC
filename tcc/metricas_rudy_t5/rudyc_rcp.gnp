#!/usr/bin/gnuplot
set terminal svg size 1000,500 enhanced fname 'arial'  fsize 6 butt solid
set output 'rudyc_rpc.svg'

min(x,y) = (x < y) ? x : y
max(x,y) = (x > y) ? x : y

notZero(x) = (x == 0) ? -0.2 : x

set key outside

set xdata time
set timefmt "%y/%m/%d-%H:%M"
set xrange ["19/03/29-00:32":"19/03/29-00:43"]

set yrange [0:0.2]
set format x "%Mm"
set format y "%fms"
set timefmt "%y/%m/%d %H:%M:%S"

set xlabel "Tempo" font "arial"
set ylabel "Tempo de Resposta" font "arial"

plot "rudyc.csv.data" using 1:(notZero($8)) title 'Regi√£o de Interesse do Personagem' with lines, \
     "rudyc.csv.data" using 1:(notZero($6)) title 'Movimento do Personagem' with lines ,\
     "rudyc.csv.data" using 1:(notZero($7)) title 'Obter Mensagens' with lines, \
     "rudyc.csv.data" using 1:(notZero($10)) title 'Enviar Mensagem' with lines, \
     "rudyc.csv.data" using 1:(min($4 * 100000000, 1)) title 'Novo cliente escalado' with impulses axes x1y1
