
set terminal svg size 400,300 enhanced fname 'arial'  fsize 6 butt solid
set output 'out.svg'

set key center top

set xdata time
set timefmt "%y/%m/%d-%H:%M"
set xrange ["19/03/20-02:35":"19/03/20-02:50"]
set yrange [0:]
set format x "%M min"
set format y "%.0fMB"
set timefmt "%y/%m/%d %H:%M:%S"

plot "data.txt" using 1:3 title 'Memória livre' with lines, \
     "data.txt" using 1:5 title 'Memória total' with linespoints, \
     "data.txt" using 1:7 title 'Memória usada' with lines
