
set terminal svg size 400,300 enhanced fname 'arial'  fsize 5 butt solid
set output 'out.svg'

set xdata time
set timefmt "%y/%m/%d-%H:%M"
set xrange ["19/03/14-23:45":"19/03/15-01:00"]
set yrange [0:]
set format x "%H:%M"
set format y "%.0f MBs"
set timefmt "%y/%m/%d %H:%M:%S"

plot "data.txt" using 1:3 title 'Memória livre' with points, \
     "data.txt" using 1:5 title 'Memória total' with points, \
     "data.txt" using 1:7 title 'Memória usada' with points
